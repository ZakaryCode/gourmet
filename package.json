{
  "name": "gourmet",
  "version": "1.0.0",
  "private": true,
  "description": "附近的美食",
  "templateInfo": {
    "name": "wxcloud",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "cloud": "npm run plugin && npm run cloudfuncs && npm run build:weapp",
    "cloudfuncs": "tsc -p cloud",
    "postcloudfuncs": "ts-node ./scripts/copy-cloud.ts",
    "plugin": "tsc -p plugins && rm -rf ./node_modules && lerna bootstrap",
    "postplugin": "ts-node ./scripts/copy-plugins.ts"
  },
  "author": "ZakaryCode",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "1.3.9",
    "@tarojs/router": "1.3.9",
    "@tarojs/taro": "1.3.9",
    "@tarojs/taro-alipay": "1.3.9",
    "@tarojs/taro-h5": "1.3.9",
    "@tarojs/taro-swan": "1.3.9",
    "@tarojs/taro-tt": "1.3.9",
    "@tarojs/taro-weapp": "^1.3.9",
    "classnames": "^2.2.6",
    "nerv-devtools": "^1.4.3",
    "nervjs": "^1.4.3",
    "wx-server-plugin": "file:lib/plugins",
    "wx-server-sdk": "^0.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@tarojs/plugin-babel": "1.3.9",
    "@tarojs/plugin-csso": "1.3.9",
    "@tarojs/plugin-sass": "1.3.9",
    "@tarojs/plugin-uglifyjs": "1.3.9",
    "@tarojs/webpack-runner": "1.3.9",
    "@types/classnames": "^2.2.9",
    "@types/fs-extra": "^8.0.0",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^12.6.2",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "acorn": "^6.2.0",
    "axios": "^0.19.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "builtin-modules": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.9",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.9",
    "fs-extra": "^8.1.0",
    "lerna": "^3.15.0",
    "mockjs": "^1.0.1-beta3",
    "ts-node": "^8.3.0",
    "typescript": "^3.0.1"
  },
  "workspaces": [
    "cloud/functions/*",
    "plugins"
  ]
}
